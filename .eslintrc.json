{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["@nuxtjs", "plugin:nuxt/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "plugins": ["check-file"],
  "rules": {
    "check-file/filename-naming-convention": [
      "error",
      {
        "components/**/*.vue": "PASCAL_CASE"
      }
    ],
    "no-else-return": 1,
    "no-console": 1
  }
}
