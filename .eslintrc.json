{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["@nuxtjs","plugin:nuxt/recommended", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@babel/eslint-parser",
        "requireConfigFile": false
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-else-return": 1
    }
}
